{
  "name": "astro-notion-blog",
  "type": "module",
  "version": "0.6.3",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:cached": "npm run cache:fetch && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint --ext .js,.ts,.astro src",
    "format": "npx prettier --write .",
    "cache:fetch": "node scripts/blog-contents-cache.cjs",
    "cache:purge": "nx reset && rm -f tmp/*",
    "_fetch-notion-blocks": "node scripts/retrieve-block-children.cjs",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.0",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/tailwind": "^5.1.4",
    "@notionhq/client": "^2.2.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@supercharge/promise-pool": "^2.4.0",
    "astro": "^5.1.2",
    "astro-icon": "^0.8.1",
    "async-retry": "^1.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "exif-be-gone": "^1.3.2",
    "katex": "^0.16.7",
    "lucide-react": "^0.469.0",
    "mermaid": "^10.9.3",
    "metascraper": "^5.34.4",
    "metascraper-description": "^5.34.4",
    "metascraper-image": "^5.34.4",
    "metascraper-title": "^5.34.4",
    "prismjs": "^1.29.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.32.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@nrwl/nx-cloud": "^15.0.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/async-retry": "^1.4.5",
    "@types/js-base64": "^3.3.1",
    "@types/katex": "^0.16.7",
    "@types/metascraper": "^5.14.1",
    "@types/metascraper-description": "^5.14.1",
    "@types/metascraper-image": "^5.14.0",
    "@types/metascraper-title": "^5.14.0",
    "@types/prismjs": "^1.26.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@typescript-eslint/typescript-estree": "^6.8.0",
    "axios": "^1.7.9",
    "cli-progress": "^3.12.0",
    "eslint": "^8.51.0",
    "eslint-plugin-astro": "^0.27.0",
    "nx": "15.5.3",
    "shadcn-ui": "^0.9.4",
    "shiki": "^3.0.0"
  },
  "nx": {
    "targets": {
      "_fetch-notion-blocks": {
        "inputs": [
          "!{projectRoot}/**/*",
          "!{projectRoot}/**/.*",
          "!{projectRoot}/**/.*/**/*",
          {
            "env": "DATABASE_ID"
          }
        ],
        "outputs": [
          "{projectRoot}/tmp"
        ]
      }
    },
    "includedScripts": [
      "_fetch-notion-blocks"
    ]
  },
  "packageManager": "pnpm@9.15.2"
}
